<div class="blackOverlay"></div>
<div class="intro">
    <div class="orbit large-9" role="region" aria-label="Favorite Space Pictures" data-orbit data-timer-delay="7500">
        <ul class="orbit-container">
            <button class="orbit-previous"><span class="show-for-sr">Previous Slide</span>&#9664;&#xFE0E;</button>
            <button class="orbit-next"><span class="show-for-sr">Next Slide</span>&#9654;&#xFE0E;</button>
            <li class="is-active orbit-slide">
                <img class="orbit-image" src="/img/story/slider/6.jpg" alt="Space">
                <figcaption class="orbit-caption">
                    {{{l.intro.slide1}}}
                </figcaption>
            </li>
            <li class="orbit-slide">
                <img class="orbit-image" src="/img/story/slider/1.jpg" alt="Space">
                <figcaption class="orbit-caption">
                    {{{l.intro.slide2}}}
                </figcaption>
            </li>
            <li class="orbit-slide">
                <img class="orbit-image" src="/img/story/slider/2.jpg" alt="Space">
                <figcaption class="orbit-caption">
                    {{{l.intro.slide3}}}
                </figcaption>
            </li>
            <li class="orbit-slide">
                <img class="orbit-image" src="/img/story/slider/5.png" alt="Space">
                <figcaption class="orbit-caption">
                    {{{l.intro.slide4}}}
                </figcaption>
            </li>
            <li class="orbit-slide">
                <img class="orbit-image" src="/img/story/slider/3.png" alt="Introduction">
                <figcaption class="orbit-caption">
                    {{{l.intro.slide5}}}
                </figcaption>
            </li>
            <li class="orbit-slide">
                <img class="orbit-image" src="/img/story/slider/4.png" alt="Introduction">
                <figcaption class="orbit-caption">
                    {{{l.intro.slide6}}}
                </figcaption>
            </li>
        </ul>
    </div>
    <br>
    <a href="/game" class="button">{{l.intro.skip}}</a>
    <br>
    <a href="{{l.intro.url}}">{{l.intro.urlButton}}</a>


</div>
